@model RegisterViewModel
@{
    ViewData["Title"] = "Register new Account";
}

@* novalidate stänger av automatisk validering som finns genom webbläsaren *@

<div class="d-fex justify-content-center">
    <div class="col-6">

        <form method="post" novalidate>
            <div class="mb-3">
                <label asp-for="FirstName"></label>
                <input />
            </div>

            <button type="submit">Register</button>
        </form>

    </div>
</div>
